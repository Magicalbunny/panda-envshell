BIN_FULL_NAME='mysql-5.6.21-linux-glibc2.5-x86_64'
BIN_DOWNLOAD_URL='http://cdn.mysql.com/archives/mysql-5.6/'$BIN_FULL_NAME'.tar.gz'
BIN_CODE_TAR=$BIN_FULL_NAME'.tar.gz'
BIN_CODE_DIR='/'$BIN_FULL_NAME

BIN_COMMON_LIB=''

INSTALL_DIR_BIN_BIN=$INSTALL_DIR_BIN'/'$BIN_FULL_NAME
INSTALL_DIR_LOG_BIN=$INSTALL_DIR_LOG'/mysql'
INSTALL_FILE_BIN_PID=$INSTALL_DIR_BIN_BIN'/mysql.pid'

BIN_CONFIGURE_VARS='INSTALL_DIR_BIN_BIN INSTALL_DIR_LOG_BIN INSTALL_FILE_BIN_PID'