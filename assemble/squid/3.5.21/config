sBinFullName='squid-3.5.21'
sBinDownloadUrl='http://www.squid-cache.org/Versions/v3/3.5/'$sBinFullName'.tar.gz'
sBinCodeTar=$sBinFullName'.tar.gz'
sBinSourceCodeSubDir='/'$sBinFullName

sBinCommonLib='gcc-c++'

sBinInstallDir=$sBinInstallRootDir'/'$sBinFullName
sBinLogInstallDir=$sLogInstallRootDir'/squid'
sBinPidInstallFile=$sBinInstallDir'/squid.pid'

BIN_VISIBLE_HOSTNAME='squid.jxulife.com'
iBinListenPort=3128
BIN_CACHE_MEN_SIZE='256 MB'
BIN_CACHE_DIR=$sBinInstallDir'/var/cache/squid'
BIN_CACHE_DIR_SIZE=512
BIN_COREDUMP_DIR=$sBinInstallDir'/var/cache/squid'
BIN_NETDB_FILENAME=$sBinInstallDir'/var/cache/squid/netdb.state'

sBinConfigVars='sBinInstallDir sBinLogInstallDir sBinPidInstallFile BIN_VISIBLE_HOSTNAME iBinListenPort BIN_CACHE_MEN_SIZE BIN_CACHE_DIR BIN_CACHE_DIR_SIZE BIN_COREDUMP_DIR BIN_NETDB_FILENAME'