sBinFullName='squid-3.5.21'
sBinDownloadUrl='http://www.squid-cache.org/Versions/v3/3.5/'$sBinFullName'.tar.gz'
sBinCodeTar=$sBinFullName'.tar.gz'
sBinSourceCodeSubDir='/'$sBinFullName

sBinCommonLib=''

sBinInstallDir=$sBinInstallRootDir'/'$sBinFullName
sBinLogInstallDir=$sLogInstallRootDir'/squid'
sBinPidInstallFile=$sBinInstallDir'/squid.pid'

sBinVisibleHostname='squid.jxulife.com'
iBinListenPort=3128
sBinCacheMemorySize='256 MB'
sBinCacheDir=$sBinInstallDir'/var/cache/squid'
sBinCacheDirSize=512
sBinCoredumpDir=$sBinInstallDir'/var/cache/squid'
sBinNetdbFilename=$sBinInstallDir'/var/cache/squid/netdb.state'

sBinConfigVars='sBinInstallDir sBinLogInstallDir sBinPidInstallFile sBinVisibleHostname iBinListenPort sBinCacheMemorySize sBinCacheDir sBinCacheDirSize sBinCoredumpDir sBinNetdbFilename'