BIN_FULL_NAME='squid-3.5.21'
BIN_DOWNLOAD_URL='http://www.squid-cache.org/Versions/v3/3.5/'$BIN_FULL_NAME'.tar.gz'
BIN_CODE_TAR=$BIN_FULL_NAME'.tar.gz'
BIN_CODE_DIR='/'$BIN_FULL_NAME

BIN_COMMON_LIB='gcc-c++'

INSTALL_DIR_BIN_BIN=$INSTALL_DIR_BIN'/'$BIN_FULL_NAME
INSTALL_DIR_LOG_BIN=$INSTALL_DIR_LOG'/squid'
INSTALL_FILE_BIN_PID=$INSTALL_DIR_BIN_BIN'/squid.pid'

BIN_VISIBLE_HOSTNAME='squid.jxulife.com'
BIN_PORT=3128
BIN_CACHE_MEN_SIZE='256 MB'
BIN_CACHE_DIR=$INSTALL_DIR_BIN_BIN'/var/cache/squid'
BIN_CACHE_DIR_SIZE=512
BIN_COREDUMP_DIR=$INSTALL_DIR_BIN_BIN'/var/cache/squid'
BIN_NETDB_FILENAME=$INSTALL_DIR_BIN_BIN'/var/cache/squid/netdb.state'

BIN_CONFIGURE_VARS='INSTALL_DIR_BIN_BIN INSTALL_DIR_LOG_BIN INSTALL_FILE_BIN_PID BIN_VISIBLE_HOSTNAME BIN_PORT BIN_CACHE_MEN_SIZE BIN_CACHE_DIR BIN_CACHE_DIR_SIZE BIN_COREDUMP_DIR BIN_NETDB_FILENAME'
