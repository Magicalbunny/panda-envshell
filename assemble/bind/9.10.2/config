sBinFullName='bind-9.10.2'
sBinDownloadUrl='ftp://ftp.isc.org/isc/bind9/9.10.2/'$sBinFullName'.tar.gz'
sBinCodeTar=$sBinFullName'.tar.gz'
sBinSourceCodeSubDir='/'$sBinFullName

sBinCommonLib='bind-utils openssl-devel'

sBinInstallDir=$sBinInstallRootDir'/'$sBinFullName
sBinLogInstallDir=$sLogInstallRootDir'/bind'
sBinPidInstallFile=$sBinInstallDir'/bind.pid'

BIN_DOMAIN='jxulife.com'

sBinConfigVars='sBinInstallDir sBinLogInstallDir sBinPidInstallFile BIN_DOMAIN'