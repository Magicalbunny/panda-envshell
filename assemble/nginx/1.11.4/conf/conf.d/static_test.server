server {
    listen 40086;
    server_name ~^(?<loader>){{BIN_STATIC_SUB_DOMAIN_REG}}\.(?<code_ver>.*)\.test\.{{BIN_BASE_DOMAIN_REG}}$;
    root {{INSTALL_DIR_APP}}/static-test/$code_ver/$loader;
    index index.html;
    add_header "Access-Control-Allow-Origin" "*";
    access_log {{INSTALL_DIR_LOG_BIN}}/static.test.server.access.log more;
    error_log {{INSTALL_DIR_LOG_BIN}}/static.test.server.error.log;
}