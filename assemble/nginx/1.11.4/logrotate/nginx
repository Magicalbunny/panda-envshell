{{sBinLogInstallDir}}/*.log {
    daily
    rotate 10
    create 0664 {{sEnvUser}} {{sEnvGroup}}
    missingok
    notifempty
    compress
    delaycompress
    postrotate
        /sbin/start-stop-daemon --stop --signal USR1 --quiet --pidfile {{sBinPidInstallFile}} --name nginx
    endscript
    sharedscripts
}