sBinFullName='memcached-1.4.34'
sBinDownloadUrl='http://www.memcached.org/files/'$sBinFullName'.tar.gz'
sBinCodeTar=$sBinFullName'.tar.gz'
sBinSourceCodeSubDir='/'$sBinFullName

sBinCommonLib='libevent-devel'

sBinInstallDir=$sBinInstallRootDir'/'$sBinFullName
sBinLogInstallDir=$sLogInstallRootDir'/memcached'
INSTALL_FILE_LOG_BIN=$sBinLogInstallDir'/memcached.log'
sBinPidInstallFile=$sBinInstallDir'/memcached.pid'

BIN_BINDIP='127.0.0.1:11211'
BIN_MAX_MEMORY=64
BIN_FACTOR=1.25

sBinConfigVars='sBinInstallDir sBinLogInstallDir INSTALL_FILE_LOG_BIN sBinPidInstallFile BIN_BINDIP BIN_MAX_MEMORY BIN_FACTOR'