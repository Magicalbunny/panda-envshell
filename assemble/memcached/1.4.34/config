sBinLogInstallFile=$sBinLogInstallDir'/memcached.log'
sBinPidInstallFile=$sBinInstallDir'/memcached.pid'

sBinBindIp='127.0.0.1'
iBinMaxMemory=64
fBinFactor=1.25
iBinTcpPort=11211
iBinUdpPort=11211
iBinMaxConn=1024

sBinConfigVars='sBinInstallDir sBinLogInstallDir sBinLogInstallFile sBinPidInstallFile sBinBindIp iBinMaxMemory fBinFactor iBinTcpPort iBinUdpPort iBinMaxConn'