server {
    listen 40084;
    server_name ~^static\.(?<dir_name>.*)\.dev\.jxu\.life$ ~^static\.(?<dir_name>.*)\.dev\.pandajingjing\.net;
    root /data/app/static_dev/$dir_name;
    index index.html;
    access_log /data/logs/nginx/static.dev.server.access.log more;
    error_log /data/logs/nginx/static.dev.server.error.log;
}
