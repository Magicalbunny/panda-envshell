server {
    listen 40081;
    server_name *.test.jxu.life *.test.pandajingjing.net;
    root /data/app/index;
    index web_test.php;

    set $backend php-test;
    if ($host ~ ^static\..*\.test\.jxu\.life$){
        set $backend static-test;
    }

    if ($host ~ ^static\..*\.test\.pandajingjing\.net$){
        set $backend static-test;
    }

    location / {
        proxy_pass http://$backend;
    }

    access_log /data/logs/nginx/proxy.test.access.log more;
    error_log /data/logs/nginx/proxy.test.error.log;
}
